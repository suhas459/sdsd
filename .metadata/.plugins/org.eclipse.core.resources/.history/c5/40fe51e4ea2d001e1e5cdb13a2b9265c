package travelTrianglePOMClasses;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.interactions.Actions;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

public class dashBoardPage {

	
	@FindBy(xpath="//a[text()='Holiday Packages']") private WebElement holiday;
	@FindBy (xpath="(//a[text()='Uttarakhand'])[1]") private WebElement kedarNath;
	public dashBoardPage (WebDriver driver) {
		PageFactory.initElements( driver, this);
	}
	
	
	public void ClickAndHoldsometimes(WebDriver driver) throws InterruptedException{
		

		Actions a= new Actions(driver);
		Thread.sleep(1500);
		a.clickAndHold(holiday).build().perform();
		Thread.sleep(1500);
		kedarNath.click();
		
		
		
		
		
		
		
	}
	
	
}
