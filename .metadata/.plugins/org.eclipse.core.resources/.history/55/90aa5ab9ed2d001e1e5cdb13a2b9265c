package travelTrianglePOMClasses;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

import utility.utility;

public class ukPage {

	
	@FindBy(xpath="(//label[@class='block AP_WEMI'])[2]") private WebElement packages;
	
	
	public ukPage(WebDriver driver) {
		PageFactory.initElements(driver, this);
	}
	
	
	public void clickOnPack(WebDriver driver) {
	
		utility.scrollIntoView(driver, packages);
		packages.click();
	}
	
	
	
	
	
	
	
	
}
